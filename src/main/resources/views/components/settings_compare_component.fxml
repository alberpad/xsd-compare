<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.compare.xsd.views.components.SettingsCompareComponent">
    <columnConstraints>
        <ColumnConstraints minWidth="50" percentWidth="20"/>
        <ColumnConstraints percentWidth="80"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>

    <Label text="%settings_shown_columns" GridPane.columnIndex="0" GridPane.rowIndex="0"
           alignment="TOP_RIGHT" maxHeight="Infinity" maxWidth="Infinity" textAlignment="RIGHT">
        <padding>
            <Insets right="5.0"/>
        </padding>
    </Label>
    <ListView fx:id="shownColumns" GridPane.columnIndex="1" GridPane.rowIndex="0">
    </ListView>
</GridPane>
